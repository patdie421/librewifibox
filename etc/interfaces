auto lo

iface lo inet loopback

auto eth0
iface eth0 inet dhcp

auto wlan1
allow-hotplug wlan1
iface wlan1 inet static
address 192.168.10.1
netmask 255.255.255.0

auto wlan0
iface wlan0 inet dhcp
   wireless-essid FreeWifi
   wireless-rate 11M
   wireless-key off
   pre-up ifconfig wlan0 mtu 1460 up
